            <ProjectItem TargetFileName="icon-60-3x.png">icon-60-3x.png</ProjectItem>
            <ProjectItem TargetFileName="icon-72.png">icon-72.png</ProjectItem>
            <ProjectItem TargetFileName="icon-72-2x.png">icon-72-2x.png</ProjectItem>
            <ProjectItem TargetFileName="icon-76.png">icon-76.png</ProjectItem>
            <ProjectItem TargetFileName="icon-76-2x.png">icon-76-2x.png</ProjectItem>
            <ProjectItem TargetFileName="icon-small.png">icon-small.png</ProjectItem>
            <ProjectItem TargetFileName="icon-small-2x.png">icon-small-2x.png</ProjectItem>
          </Folder>
          <Folder Name="windows">
            <ProjectItem TargetFileName="Square150x150Logo.scale-100.png">Square150x150Logo.scale-100.png</ProjectItem>
            <ProjectItem TargetFileName="Square30x30Logo.scale-100.png">Square30x30Logo.scale-100.png</ProjectItem>
            <ProjectItem TargetFileName="Square310x310Logo.scale-100.png">Square310x310Logo.scale-100.png</ProjectItem>
            <ProjectItem TargetFileName="Square150x150Logo.scale-240.png">Square150x150Logo.scale-240.png</ProjectItem>
