<VSTemplate Version="3.0.0" Type="Project" xmlns="http://schemas.microsoft.com/developer/vstemplate/2005">
  <TemplateData>
    <Name _locID="Cordova_JS_Blank_Name">Blank App (Apache Cordova)</Name>
    <Description _locID="Cordova_JS_Blank_Desc">A blank project that uses Apache Cordova™ to help you build an app that targets Android, iOS, and the Windows Universal Platform.</Description>
    <Icon>BlankCordovaJS.ico</Icon>
    <ProjectType>JavaScript</ProjectType>
    <TemplateID>Microsoft.JavaScript.CordovaFileMirrorBlankJS</TemplateID>
    <SortOrder>1</SortOrder>
    <CreateNewFolder>false</CreateNewFolder>
    <DefaultName>BlankCordovaApp</DefaultName>
    <ProvideDefaultName>true</ProvideDefaultName>
    <NumberOfParentCategoriesToRollUp>2</NumberOfParentCategoriesToRollUp>
    <PromptForSaveOnCreation>true</PromptForSaveOnCreation>
    <TargetPlatformName>Windows</TargetPlatformName>
	<CreateInPlace>true</CreateInPlace>
  </TemplateData>
  <TemplateContent>
    <Project File="Application.jsproj" ReplaceParameters="true">
      <ProjectItem TargetFileName="bower.json" ReplaceParameters="true">bower.json</ProjectItem>
      <ProjectItem TargetFileName="package.json" ReplaceParameters="true">package.json</ProjectItem>
      <ProjectItem TargetFileName="build.json" ReplaceParameters="true">build.json</ProjectItem>
      <ProjectItem TargetFileName="config.xml" ReplaceParameters="true">config.xml</ProjectItem>
      <ProjectItem TargetFileName=".gitignore" ReplaceParameters="true">.gitignore</ProjectItem>
      <Folder Name="www">
        <ProjectItem TargetFileName="index.html" ReplaceParameters="true">index.html</ProjectItem>
        <Folder Name="css">
          <ProjectItem TargetFileName="index.css">index.css</ProjectItem>
        </Folder>
        <Folder Name="images">
          <ProjectItem TargetFileName="cordova.png">cordova.png</ProjectItem>
        </Folder>
        <Folder Name="scripts">
          <ProjectItem TargetFileName="index.js" ReplaceParameters="true">index.js</ProjectItem>
          <ProjectItem TargetFileName="platformOverrides.js">platformOverrides.js</ProjectItem>
        </Folder>
      </Folder>
      <Folder Name="merges">
        <Folder Name="android">
          <Folder Name="scripts">
            <ProjectItem TargetFileName="platformOverrides.js">platformOverrides.js</ProjectItem>
            <ProjectItem TargetFileName="android2.3-jscompat.js">android2.3-jscompat.js</ProjectItem>
          </Folder>
        </Folder>
        <Folder Name="ios"/>
        <Folder Name="windows">
          <Folder Name="scripts">
            <ProjectItem TargetFileName="platformOverrides.js">platformOverrides.js</ProjectItem>
            <ProjectItem TargetFileName="winstore-jscompat.js">winstore-jscompat.js</ProjectItem>
          </Folder>
        </Folder>
      </Folder>
      <Folder Name="res">
        <Folder Name="icons">
          <Folder Name="android">
            <ProjectItem TargetFileName="icon-36-ldpi.png">icon-36-ldpi.png</ProjectItem>
            <ProjectItem TargetFileName="icon-48-mdpi.png">icon-48-mdpi.png</ProjectItem>
            <ProjectItem TargetFileName="icon-72-hdpi.png">icon-72-hdpi.png</ProjectItem>
            <ProjectItem TargetFileName="icon-96-xhdpi.png">icon-96-xhdpi.png</ProjectItem>
          </Folder>
          <Folder Name="ios">
            <ProjectItem TargetFileName="icon-40.png">icon-40.png</ProjectItem>
            <ProjectItem TargetFileName="icon-40-2x.png">icon-40-2x.png</ProjectItem>
            <ProjectItem TargetFileName="icon-50.png">icon-50.png</ProjectItem>
            <ProjectItem TargetFileName="icon-50-2x.png">icon-50-2x.png</ProjectItem>
            <ProjectItem TargetFileName="icon-57.png">icon-57.png</ProjectItem>
