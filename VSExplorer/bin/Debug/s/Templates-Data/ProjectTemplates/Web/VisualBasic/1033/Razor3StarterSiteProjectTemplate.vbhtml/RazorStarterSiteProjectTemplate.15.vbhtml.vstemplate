            <Folder Name="Scripts" />
            <ProjectItem>_AppStart.vbhtml</ProjectItem>
            <ProjectItem>_SiteLayout.vbhtml</ProjectItem>
            <ProjectItem>About.vbhtml</ProjectItem>
            <ProjectItem>Contact.vbhtml</ProjectItem>
            <ProjectItem>Default.vbhtml</ProjectItem>
            <ProjectItem>favicon.ico</ProjectItem>
            <ProjectItem ReplaceParameters="true" TargetFileName="Web.config">Web12.config</ProjectItem>
        </Project>
    </TemplateContent>
