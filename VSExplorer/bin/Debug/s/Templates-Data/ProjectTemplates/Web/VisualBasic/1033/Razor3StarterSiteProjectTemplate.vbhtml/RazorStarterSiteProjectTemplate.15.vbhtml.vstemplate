                <ProjectItem>Register.vbhtml</ProjectItem>
                <ProjectItem>RegisterService.vbhtml</ProjectItem>
                <ProjectItem>Thanks.vbhtml</ProjectItem>
            </Folder>
            <Folder Name="App_Data">
                <ProjectItem>StarterSite.sdf</ProjectItem>
            </Folder>
            <Folder Name="Content">
                <ProjectItem>Site.css</ProjectItem>
